#include <iostream>
using namespace std;

int main() {
	setlocale(LC_ALL, "Russian");
	int N;
	cout << "N = ";
	cin >> N;
	double* A = new double[N];
	for (int i = 0; i < N; i++)
		A[i] = i+1;
	cout << A[0] << endl;
	cout << A[N-1] << endl;
	cout << A[1] << endl;
	cout << A[N-2] << endl;
	cout << A[2] << endl;
	cout << A[N-3] << endl;
	delete[] A;
	return 0;
}
