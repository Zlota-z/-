#include <iostream>

using namespace std;

double PowerA3(double A)
{
	double B = A * A * A;
	return B;
}

int main() {
	setlocale(LC_ALL, "Russian");
	double a;
	for (int i = 1; i < 6; i++)
	{
		cout << "Введите число: ";
		cin >> a;
		cout << "Третья степень числа " << a << " равна " << PowerA3(a) << endl;
	}
	return 0;
}
