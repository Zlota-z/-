#include <iostream>
#include <math.h>

using namespace std;

int main()
{
    setlocale(LC_ALL, "Russian");
    int a, b, c;
    bool bo;
    cout << "A = ";
    cin >> a;
    cout << "B = ";
    cin >> b;
    cout << "C = ";
    cin >> c;
    bo = ((a < b) & (b < c));
    cout << bo << endl;
    system("pause");
    return 0;
}
